{
  "name": "georacer",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "hybrid": "concurrently \"npm run dev\" \"nodemon ./backend/dist/main.js\"",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest src --config jest.config.json",
    "test:watch": "npm run test -- --watch"
  },
  "devDependencies": {
    "@sveltejs/kit": "next",
    "@types/googlemaps": "^3.43.3",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "autoprefixer": "^10.4.0",
    "concurrently": "^6.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "svelte": "^3.42.6",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.9.4",
    "tailwindcss": "^2.2.19",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3",
    "@babel/core": "^7.14.0",
    "@babel/preset-env": "^7.14.0",
    "jest": "^27.0.0",
    "babel-jest": "^27.0.0",
    "svelte-jester": "^2.0.1",
    "@testing-library/svelte": "^3.0.0",
    "@testing-library/jest-dom": "^5.14.0",
    "ts-jest": "^27.0.0",
    "@types/jest": "^27.0.0",
    "@types/testing-library__jest-dom": "^5.14.0"
  },
  "type": "module",
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.12.8",
    "api-client": "github:GeoGameOrg/api-client",
    "google-maps": "^4.3.3",
    "nodemon": "^2.0.14",
    "socket.io-client": "^4.3.2",
    "ts-node": "^10.4.0"
  }
}
